ssh -Y elephant@192.168.0.7

For ($i=1; $i -le 24; $i++) {
    $ip = "192.168.0." + $i
    Write-Host "Trying to connect to $ip"
    ssh -o ConnectTimeout=5 -o StrictHostKeyChecking=no -Y elephant@$ip
}

